{
  "name": "codefee-kit",
  "version": "0.0.13",
  "description": "A simple UI kit for React projects",
  "main": "src/index.ts",
  "scripts": {
    "build": "webpack --env prod",
    "dev": "webpack-dev-server --open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "clean-dist": "rimraf dist",
    "ts-build": "tsc && tsc -m es6 --outDir dist/lib-esm",
    "package-info": "node scripts/package.ts",
    "pack": "npm run clean-dist && npm run build && npm run package-info",
    "publish": "npm run pack &&  npm publish dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DriLLFreAK100/codefee-kit.git"
  },
  "keywords": [
    "Codefee",
    "Codefee-Kit",
    "React",
    "React Component Library"
  ],
  "author": "drillfreak100",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DriLLFreAK100/codefee-kit/issues"
  },
  "homepage": "https://github.com/DriLLFreAK100/codefee-kit#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addons": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@storybook/theming": "^6.0.27",
    "@types/lodash": "^4.14.162",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "husky": "^4.3.0",
    "mini-css-extract-plugin": "^0.11.3",
    "postcss": "^8.1.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
